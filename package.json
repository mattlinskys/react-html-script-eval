{
  "name": "react-html-script-eval",
  "version": "1.0.0",
  "description": "React hook to execute or load scripts inside element with html provided by dangerouslySetInnerHTML",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "clean": "rimraf ./dist",
    "prepublish": "npm run clean && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mattlinskys/react-html-script-eval.git"
  },
  "keywords": [
    "react",
